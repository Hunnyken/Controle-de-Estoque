program Estoque;

uses
  Vcl.Forms,
  unitPrincipal in 'unitPrincipal.pas' {formPrincipal},
  unitCadProduto in 'view\unitCadProduto.pas' {formCadProduto},
  unitCadMovimentacao in 'view\unitCadMovimentacao.pas' {formCadMovimentacao},
  unitConsMovimentacao in 'view\unitConsMovimentacao.pas' {formConsMovimentacao};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TformPrincipal, formPrincipal);
  Application.CreateForm(TformCadProduto, formCadProduto);
  Application.CreateForm(TformCadMovimentacao, formCadMovimentacao);
  Application.CreateForm(TformConsMovimentacao, formConsMovimentacao);
  Application.Run;
end.
